{
  "name": "dotbook",
  "displayName": "dotbook",
  "description": "Ace editor view for dothttp notebook output renderer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "dotbook",
        "entrypoint": "./out/client/index.js",
        "displayName": "Dotbook",
        "mimeTypes": [
          "x-application/dotbook"
        ],
        "requiresMessaging": "always"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@cedric05/preact-ace": "^10.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/uuid": "^8.3.0",
    "@types/vkbeautify": "^0.99.2",
    "@types/vscode": "^1.68.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/codicons": "0.0.22",
    "@vscode/test-electron": "^2.1.3",
    "css-loader": "^4.2.0",
    "eslint": "^8.16.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^8.0.3",
    "http-status-codes": "^2.1.4",
    "mocha": "^10.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "vkbeautify": "^0.99.3",
    "vscode-notebook-error-overlay": "^1.0.1",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "svg-inline-loader":"^0.8.2"
  }
}
